    "@vue/compiler-sfc": "^3.2.0",
    "jest": "^27.0.6",
    "prettier": "^2.3.2",
    "sass": "^1.37.5",
    "ts-jest": "^27.0.4",
    "typescript": "^4.3.2",
    "vite": "^2.4.4",
    "vue-tsc": "^0.2.2"
  },
  "jest": {
    "roots": [
      "<rootDir>/src"
    ],
    "testMatch": [ "**/__tests__/ut/**/*.+(ts|tsx|js)" ],
    "transform": {
      "^.+\\.(ts|tsx)$": "ts-jest"
