  "version": "0.0.0",
  "scripts": {
    "dev": "vite",
    "build": "vue-tsc --noEmit && vite build",
    "serve": "vite preview",
    "test": "jest"
  },
  "dependencies": {
    "vue": "^3.2.0"
  },
  "devDependencies": {
